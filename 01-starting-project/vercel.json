{
  "version": 2,
  "builds": [
    {
      "src": "01-starting-project/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "01-starting-project/dist/<project-name>"
      }
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
